    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 secure-mint-engine",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.60.0",
    "@graphprotocol/graph-ts": "0.31.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.2"
  }
