{
  "name": "secure-mint-engine-subgraph",
  "version": "1.0.0",
  "description": "The Graph subgraph for Secure Mint Engine",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/secure-mint-engine"
  },
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
