{
  "detectors_to_run": [
    "abiencoderv2-array",
    "arbitrary-send-erc20",
    "arbitrary-send-erc20-permit",
    "arbitrary-send-eth",
    "array-by-reference",
    "controlled-array-length",
    "controlled-delegatecall",
    "delegatecall-loop",
    "encode-packed-collision",
    "incorrect-equality",
    "incorrect-shift",
    "locked-ether",
    "mapping-deletion",
    "msg-value-loop",
