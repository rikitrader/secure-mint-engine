  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "exclude_optimization": true,
  "filter_paths": [
    "node_modules",
    "lib",
    "test",
    "script"
  ],
  "show_ignored_findings": false,
  "legacy_ast": false,
  "json": "slither-report.json",
  "sarif": "slither-report.sarif",
  "checklist": true,
  "fail_on": "high",
  "printers_to_run": [
    "contract-summary",
    "function-summary",
    "human-summary"
  ],
  "markdown_root": "https://github.com/your-org/secure-mint-engine/blob/main/",
  "compile_force_framework": "hardhat",
  "solc_remaps": [
    "@openzeppelin/=node_modules/@openzeppelin/",
    "@chainlink/=node_modules/@chainlink/"
  ],
  "exclude_dependencies": true,
  "triage_database": ".slither-triage.json",
  "triage_mode": true
}
