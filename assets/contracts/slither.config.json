    "public-mappings-nested",
    "reentrancy-benign",
    "reentrancy-eth",
    "reentrancy-events",
    "reentrancy-no-eth",
    "reentrancy-unlimited-gas",
    "return-bomb",
    "return-leave",
    "shadowing-abstract",
    "shadowing-state",
    "storage-array",
    "suicidal",
    "tx-origin",
    "unchecked-lowlevel",
    "unchecked-send",
    "unchecked-transfer",
    "uninitialized-local",
    "uninitialized-state",
    "uninitialized-storage",
    "unprotected-upgrade",
    "unused-return",
    "variable-scope",
    "void-cst",
